{% extends ':templates:box-with-footer.html.twig' %}

{% form_theme form "bootstrap_3_layout.html.twig" %}

{% block title %}{{ 'user.student.import.title' | trans }}{% endblock %}
{% block box_title %}{{ 'user.student.import.title' | trans }}{% endblock %}

{% block box_form_start %}
    {{ form_start(form) }}
{% endblock %}

{% block box_body_content %}
    {{ form_widget(form) }}
{% endblock %}

{% block box_footer %}
    {{ action.import_save("user.student.import.submit" | trans) }}
{% endblock %}

{% block box_form_end %}
    {{ form_end(form) }}
{% endblock %}
