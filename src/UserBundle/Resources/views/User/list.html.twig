{% extends "::base.html.twig" %}


{% block body %}


    <div class="box">
        <div class="box-header">
            <div class="box-actions pull-right">
                <a href="{{ path('user_management_new') }}" class="btn btn-primary">
                    <i class="glyphicon glyphicon-plus"></i>
                    {{ "layout.user.add" | trans }}
                </a>
            </div>
            <h1 class="box-title">
                {{ "layout.user.list" | trans }}
            </h1>
        </div>
        <div class="box-body">
            {{ datatable_render_html(datatable) }}
        </div>
    </div>


{% endblock body %}

{% block javascripts %}
    <script>
        $ = jQuery;
    </script>
    {{ datatable_render_js(datatable) }}
{% endblock %}
