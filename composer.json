{
    "name": "he8us/das",
    "description": "Gestion des remédiations et donne la possibilité aux jeunes d'êtres proactifs dans leurs études",
    "version": "0.1.0",
    "keywords": [
        "docker",
        "symfony",
        "react",
        "redux",
        "webpack",
        "babel",
        "eslint"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric Michaux"
        }
    ],
    "support": {
        "issues": "https://github.com/he8us/DAS/issues"
    },
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "test/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-mcrypt": "*",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "ext-exif": "*",
        "symfony/symfony": "3.1.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/translation-bundle": "dev-master",
        "jms/di-extra-bundle": "^1.7",
        "twig/extensions": "^1.3",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "dzunke/feature-flags-bundle": "^2.1",
        "vich/uploader-bundle": "^1.3",
        "stof/doctrine-extensions-bundle": "^1.2",
        "oneup/flysystem-bundle": "^1.4",
        "liip/imagine-bundle": "^1.6",
        "phpcollection/phpcollection": "^0.5.0",
        "friendsofsymfony/jsrouting-bundle": "^1.6",
        "sg/datatablesbundle": "^0.12.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "trappar/alice-generator-bundle": "^1.0",
        "hautelook/alice-bundle": "1.3.1",
        "nelmio/alice": "2.2.2",
        "he8us/feedback-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "doctrine/data-fixtures": "^1.2",
        "theofidry/alice-bundle-extension": "^1.3",
        "behat/behat": "^3.2",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "test",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
